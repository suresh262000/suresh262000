        var object = { status: false, ele: null };
        function delete_emp(ev) {


            if (object.status) { return true; };

            swal({
                title: "Are you sure?",
                text: "You won't be able to revert this!",
                type: "warning",
                showCancelButton: true,
                confirmButtonColor: '#e91d1d',
                cancelButtonColor: '#2a7de5',
                confirmButtonText: "Yes, delete it!",
                cancelButtonText: "Cancel",
                closeOnConfirm: true
            },
                function () {
                    object.status = true;
                    object.ele = ev;
                    object.ele.click();
                });

            return false;
        };            </script>    <div>    <h2 class="header">               <i -feather="check-square"></i>            Projects           </h2>    <div>        <button type="button" class="btn button-1 createmodal" data-toggle="modal" data-target="#createmodal">            + Create Project        </button>     <%--<a class="pull-right btn button-1" OnClick="excel_Click1" ID="LinkButton3" runat="server">Download Report</a>--%>        <asp:LinkButton class="pull-right btn button-1" OnClick="excel_Click1" ID="LinkButton2" runat="server" ToolTip="Click">Download Report</asp:LinkButton>    <%--<asp:LinkButton ID="LinkButton2" class="pull-right btn button-1"  runat="server" OnClick="excel_Click1">Download Report</asp:LinkButton>--%>    </div>               <div class="table-responsive">        <br />     <asp:GridView ID="payRollTable" EmptyDataText="No Data Available" runat="server" CssClass="table table-striped  dt-responsive nowrap datatable1" DataKeyNames="Project_Id" width="100%" cellspacing="0" AutoGenerateColumns ="false" >           <%-- Theme Properties --%>         <Columns>                      <%-- <asp:HiddenField DataField ="" HeaderText ="Payroll ID" />--%>            <asp:BoundField DataField ="Project_Id" HeaderText ="Project ID" />            <asp:BoundField DataField ="Project_Name" HeaderText ="Project Name" />            <%--<asp:HiddenField ID="Payroll_IDD" runat="server" Value='<%# Bind("Payroll_IDD") %>'/>--%>            <asp:BoundField DataField ="Resource" HeaderText ="No of Resources" />            <asp:BoundField DataField ="Start_Date" HeaderText ="Start Date" DataFormatString = "{0:dd/MM/yyyy}" />            <asp:BoundField DataField ="End_Date" HeaderText ="End Date" DataFormatString = "{0:dd/MM/yyyy}" />            <asp:BoundField DataField ="status" HeaderText ="Status" />                <asp:BoundField DataField ="Created_By" HeaderText ="Created By" />                <asp:BoundField DataField ="is